{
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "react/jsx-indent": [
            "error",
            4
        ],
        "indent": [
            2,
            4
        ],
        "template-curly-spacing": [
            2,
            "always"
        ],
        "object-curly-spacing": [
            2,
            "always"
        ],
        "no-multiple-empty-lines": [
            2,
            {
                "max": 1,
                "maxEOF": 0,
                "maxBOF": 0
            }
        ]
    }
}